{
  "name": "andrew-svirin/ebics-client-php",
  "type": "library",
  "description": "PHP library to communicate with bank through EBICS protocol.",
  "keywords": [
    "ebics",
    "PHP",
    "client",
    "symfony",
    "openssl",
    "rsa",
    "x509"
  ],
  "authors": [
    {
      "name": "Andrew Svirin"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ext-dom": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-json": "*",
    "phpseclib/phpseclib": "~2.0",
    "symfony/http-client": "^4.3||^5.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.29",
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "AndrewSvirin\\Ebics\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndrewSvirin\\Ebics\\Tests\\": "tests"
    }
  },
  "suggest": {
    "silarhi/cfonb-parser": "If you need to parse CFONB datas from French Bank",
    "andrew-svirin/mt942-php": "If you need to parse format MT942 from VMK, STA requests."
  },
  "prefer-stable": true
}

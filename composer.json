{
  "name": "andrew-svirin/ebics-client-php",
  "version": "1.0.2",
  "type": "library",
  "description": "PHP library to communicate with bank through EBICS protocol.",
  "keywords": [
    "ebics",
    "PHP",
    "client",
    "symfony",
    "openssl",
    "rsa",
    "x509"
  ],
  "authors": [
    {
      "name": "Andrew Svirin"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ext-dom": "*",
    "ext-openssl": "*",
    "comodojo/httprequest": "~1.0",
    "phpseclib/phpseclib": "~2.0",
    "ext-zlib": "*",
    "ext-json": "*",
    "symfony/http-client": "^4.3||^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "AndrewSvirin\\Ebics\\": "src"
    }
  }
}
